<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.buddheshwar.internshiparoma">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data android:name="com.google.android.geo.API_KEY"
            android:value="@string/GOOGLE_PLACE_API_KEY"/>

        <activity android:name=".SelectCity" />
        <activity android:name=".ContactUs" />
        <activity android:name=".AboutUs" />
        <activity
            android:name=".NotificationActivity"
            android:theme="@style/SecondAppTheme" />
        <activity android:name=".PostInternshipActivity" />
        <activity
            android:name=".GettingStartedActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AsEmployer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SelectField"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AsIntern2"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AsIntern"
            android:screenOrientation="portrait" />
        <activity android:name=".ContinueAsActivity" />
        <activity android:name=".resume.Resume" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".EditPreferencesActivity" />
        <activity android:name=".VerifyEmailActivity" />
        <activity android:name=".SplashScreen">


            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".Mainscreen" />
    </application>

</manifest>